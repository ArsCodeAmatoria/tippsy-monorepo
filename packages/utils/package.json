{
  "name": "@tippsy/utils",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "bun run tsup src/index.ts --format esm,cjs --dts",
    "dev": "bun run tsup src/index.ts --format esm,cjs --watch --dts",
